L_docket_date_reargument_denied": null, "CL_docket_blocked": false, "CL_docket_source": 0, "CL_docket_date_blocked": null, "CL_docket_case_name_full": "", "CL_docket_date_last_filing": null, "CL_docket_slug": "city-of-miami-v-wright", "CL_docket_court": {"CL_court_start_date": null, "CL_court_end_date": null, "CL_court_resource_uri": "http://www.courtlistener.com/api/rest/v3/courts/fladistctapp/", "CL_court_position": 355.0, "CL_court_url": "http://www.flcourts.org/courts/dca/dca.shtml", "CL_court_jurisdiction": "SA", "CL_court_has_oral_argument_scraper": false, "CL_court_date_modified": "2014-08-01T16:49:33.319000Z", "CL_court_short_name": "District Court of Appeal of Florida", "CL_court_citation_string": "Fla. Dist. Ct. App.", "CL_court_in_use": true, "CL_court_has_opinion_scraper": true, "CL_court_full_name": "District Court of Appeal of Florida"}, "CL_docket_case_name_short": "Wright", "CL_docket_date_cert_granted": null, "CL_docket_cause": null, "CL_docket_absolute_url": "/docket/1343859/city-of-miami-v-wright/", "CL_docket_pacer_case_id": null, "CL_docket_date_argued": null, "CL_docket_date_terminated": null, "CL_docket_audio_files": [], "CL_docket_jury_demand": null}, "CL_cluster_lexis_cite": "", "CL_cluster_state_cite_two": "", "CL_cluster_slug": "city-of-miami-v-wright", "CL_cluster_citation_count": 3, "CL_cluster_precedential_status": "Published", "CL_cluster_state_cite_regional": "414 So. 2d 24", "CL_cluster_sub_opinions": ["http://www.courtlistener.com/api/rest/v3/opinions/1765120/"], "CL_cluster_absolute_url": "/opinion/1765120/city-of-miami-v-wright/", "CL_cluster_scdb_decision_direction": null, "CL_cluster_westlaw_cite": "", "CL_cluster_state_cite_one": "", "CL_cluster_procedural_history": "", "CL_cluster_date_blocked": null, "CL_cluster_specialty_cite_one": "", "CL_cluster_case_name_full": "", "CL_cluster_federal_cite_two": "", "CL_cluster_nature_of_suit": "", "CL_cluster_scotus_early_cite": "", "CL_cluster_date_filed": "1982-05-18", "CL_cluster_federal_cite_one": "", "CL_cluster_citation_id": 1737891, "CL_cluster_scdb_id": "", "CL_cluster_syllabus": "", "CL_cluster_panel": [], "CL_cluster_federal_cite_three": "", "CL_cluster_posture": "", "CL_cluster_case_name": "City of Miami v. Wright", "CL_cluster_non_participating_judges": [], "CL_cluster_source": "L", "CL_cluster_blocked": false, "CL_cluster_state_cite_three": "", "CL_cluster_resource_uri": "http://www.courtlistener.com/api/rest/v3/clusters/1765120/", "CL_cluster_date_created": "2014-10-30T16:00:50.817402Z"}, "Opinion_Date": "1982-05-18 00:00:00", "ML_Tags_Text": [], "Cases_Cited_Link": ["http://www.courtlistener.com/api/rest/v3/opinions/1715089/", "http://www.courtlistener.com/api/rest/v3/opinions/1753917/"], "CL_opinion_absolute_url": "/opinion/1765120/city-of-miami-v-wright/", "CL_opinion_extracted_by_ocr": false, "Rule_Tags": "", "Manual_Tags": [], "Title": "City of Miami v. Wright", "PartyType1": "", "Document_Level_Tags": ["ScrapeSource_CL", "ScrapeDate_06202018"], "Case_Text": "", "TF_IDF_Tags": [], "ML_Tags_Doc_Level": [], "Data_source": "Court Listener", "CL_opinion_type": "010combined", "Duplicate": "No", "HTMLtext": "<div>\n<center><b><span class=\"citation no-link\"><span class=\"volume\">414</span> <span class=\"reporter\">So. 2d</span> <span class=\"page\">24</span></span> (1982)</b></center>\n<center><h1>CITY OF MIAMI, Appellant,<br>\nv.<br>\nRobert X. WRIGHT (Deceased) and Earline M. Wright, Appellees.</h1></center>\n<center>No. AG-285.</center>\n<center><p><b>District Court of Appeal of Florida, First District.</b></p></center>\n<center>May 18, 1982.</center>\n<p><span class=\"star-pagination\">*25</span> George F. Knox, Jr., City Atty. and Joseph Hackney, Jr., Asst. City Atty., Miami, for appellant.</p>\n<p>Sam H. Nedelman, Miami, for appellees.</p>\n<p>PER CURIAM.</p>\n<p>The City of Miami urges that the Deputy Commissioner erred in awarding to the claimant attorney's fees. We agree. Since the City of Miami admitted that the claimant, now deceased, suffered a compensable injury and paid to him certain Workers' Compensation benefits, there is no basis for assessing attorney's fees pursuant to Section 440.34(2)(c), Florida Statutes (1979). <i>Four Quarters Habitat, Inc. v. Miller,</i> <span class=\"citation\" data-id=\"1753917\"><a href=\"/opinion/1753917/four-quarters-habitat-inc-v-miller/\"><span class=\"volume\">405</span> <span class=\"reporter\">So. 2d</span> <span class=\"page\">475</span></a></span> (Fla. 1st DCA 1981). <i>Dolphin Tire Co. v. Ellison,</i> <span class=\"citation\" data-id=\"1715089\"><a href=\"/opinion/1715089/dolphin-tire-co-v-ellison/\"><span class=\"volume\">402</span> <span class=\"reporter\">So. 2d</span> <span class=\"page\">36</span></a></span> (Fla. 1st DCA 1981) contains no support for the claimant's argument, for the carrier in that case denied that a compensable catastrophic injury, as defined by Section 440.15(2)(b), Florida Statutes (1979), occurred. In fact, <i>Dolphin Tire</i> specifically states that it should not be construed to authorize benefits where the employer/carrier does not deny that an injury occurred and the claimant is not required to prove his injury and entitlement to compensation. <i>Id.,</i> at 37.</p>\n<p>REVERSED.</p>\n<p>McCORD, MILLS and SHIVERS, JJ., concur.</p>\n</div>", "CL_opinion_per_curiam": true}