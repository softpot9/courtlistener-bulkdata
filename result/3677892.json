_joined_by": [], "ML_Tags": [], "URL": null, "Searchtags": [], "Cases_Cited": ["", "", "", "", ""], "Party2": ". Pinecrest Manor", "Year": 1928, "CL_opinion_cluster": {"CL_cluster_scdb_votes_majority": null, "CL_cluster_neutral_cite": "", "CL_cluster_judges": "STACY, C. J.", "CL_cluster_scdb_votes_minority": null, "CL_cluster_attorneys": "U. L. Spence for plaintiff .\n\n H. F. Seawell  Son for defendant .", "CL_cluster_date_modified": "2017-04-15T01:12:19.430322Z", "CL_cluster_docket": {"CL_docket_assigned_to": null, "CL_docket_assigned_to_str": "", "CL_docket_resource_uri": "http://www.courtlistener.com/api/rest/v3/dockets/3803347/", "CL_docket_referred_to": null, "CL_docket_clusters": ["http://www.courtlistener.com/api/rest/v3/clusters/3931205/"], "CL_docket_case_name": "Pickler v. . Pinecrest Manor", "CL_docket_nature_of_suit": "", "CL_docket_date_reargued": null, "CL_docket_docket_number": "", "CL_docket_date_modified": "2016-07-06T06:24:04.630983Z", "CL_docket_filepath_ia": "", "CL_docket_date_filed": null, "CL_docket_filepath_local": null, "CL_docket_jurisdiction_type": "", "CL_docket_date_cert_denied": null, "CL_docket_date_created": "2016-07-06T06:24:04.630958Z", "CL_docket_date_reargument_denied": null, "CL_docket_blocked": false, "CL_docket_source": 4, "CL_docket_date_blocked": null, "CL_docket_case_name_full": "J. W. Pickler and J. W. Lampley, Copartners Trading and Doing Business Under the Firm Name of Pickler-Lampley Construction Company v. Pinecrest Manor, Incorporated.", "CL_docket_date_last_filing": null, "CL_docket_slug": "pickler-v-pinecrest-manor", "CL_docket_court": {"CL_court_start_date": "1799-01-01", "CL_court_end_date": null, "CL_court_resource_uri": "http://www.courtlistener.com/api/rest/v3/courts/nc/", "CL_court_position": 366.97, "CL_court_url": "http://www.nccourts.org/courts/appellate/supreme/", "CL_court_jurisdiction": "S", "CL_court_has_oral_argument_scraper": false, "CL_court_date_modified": "2016-09-08T20:38:41.131652Z", "CL_court_short_name": "Supreme Court of North Carolina", "CL_court_citation_string": "N.C.", "CL_court_in_use": true, "CL_court_has_opinion_scraper": true, "CL_court_full_name": "Supreme Court of North Carolina"}, "CL_docket_audio_files": [], "CL_docket_case_name_short": "Pickler", "CL_docket_date_cert_granted": null, "CL_docket_cause": "", "CL_docket_absolute_url": "/docket/3803347/pickler-v-pinecrest-manor/", "CL_docket_pacer_case_id": "", "CL_docket_date_argued": null, "CL_docket_date_terminated": null, "CL_docket_referred_to_str": "", "CL_docket_jury_demand": ""}, "CL_cluster_lexis_cite": "", "CL_cluster_state_cite_two": "", "CL_cluster_slug": "pickler-v-pinecrest-manor", "CL_cluster_citation_count": 1, "CL_cluster_precedential_status": "Published", "CL_cluster_case_name_short": "Pickler", "CL_cluster_sub_opinions": ["http://www.courtlistener.com/api/rest/v3/opinions/3677892/"], "CL_cluster_absolute_url": "/opinion/3931205/pickler-v-pinecrest-manor/", "CL_cluster_scdb_decision_direction": null, "CL_cluster_westlaw_cite": "", "CL_cluster_state_cite_one": "195 N.C. 614", "CL_cluster_procedural_history": "", "CL_cluster_date_blocked": null, "CL_cluster_specialty_cite_one": "", "CL_cluster_case_name_full": "J. W. Pickler and J. W. Lampley, Copartners Trading and Doing Business Under the Firm Name of Pickler-Lampley Construction Company v. Pinecrest Manor, Incorporated.", "CL_cluster_date_filed_is_approximate": false, "CL_cluster_nature_of_suit": "", "CL_cluster_federal_cite_two": "", "CL_cluster_scotus_early_cite": "", "CL_cluster_date_filed": "1928-05-09", "CL_cluster_federal_cite_one": "", "CL_cluster_citation_id": null, "CL_cluster_source": "Z", "CL_cluster_scdb_id": "", "CL_cluster_syllabus": "", "CL_cluster_panel": [], "CL_cluster_federal_cite_three": "", "CL_cluster_posture": "APPEAL by defendant from  Oglesby, J. , at December Term, 1927, of MOORE.", "CL_cluster_case_name": "Pickler v. . Pinecrest Manor", "CL_cluster_non_participating_judges": [], "CL_cluster_state_cite_regional": "143 S.E. 8", "CL_cluster_blocked": false, "CL_cluster_state_cite_three": "", "CL_cluster_resource_uri": "http://www.courtlistener.com/api/rest/v3/clusters/3931205/", "CL_cluster_date_created": "2016-07-06T06:24:04.637130Z"}, "Opinion_Date": "1928-05-09 00:00:00", "ML_Tags_Text": [], "Cases_Cited_Link": ["http://www.courtlistener.com/api/rest/v3/opinions/3644530/", "http://www.courtlistener.com/api/rest/v3/opinions/3644667/", "http://www.courtlistener.com/api/rest/v3/opinions/3654635/", "http://www.courtlistener.com/api/rest/v3/opinions/3661845/", "http://www.courtlistener.com/api/rest/v3/opinions/3669520/"], "CL_opinion_absolute_url": "/opinion/3931205/pickler-v-pinecrest-manor/", "CL_opinion_extracted_by_ocr": false, "Rule_Tags": "", "Manual_Tags": [], "Title": "Pickler v. . Pinecrest Manor", "PartyType1": "", "Document_Level_Tags": ["ScrapeSource_CL", "ScrapeDate_06202018"], "Case_Text": "", "TF_IDF_Tags": [], "ML_Tags_Doc_Level": [], "Data_source": "Court Listener", "CL_opinion_type": "020lead", "Duplicate": "No", "HTMLtext": "<p>Civil action to recover $5,567.01, balance alleged to be due on account of the construction or erection of eight cottages desired by the defendant for use in connection with its sanatorium situate near the town of Southern Pines, N.C.</p>\n<p>Upon denial of liability and counterclaim filed by the defendant for damages occasioned by delay, etc., the cause was referred, apparently by consent, to Hon. D. B. Teague, who found the facts and reported same, together with his conclusions of law, to the court, allowing the plaintiff a recovery, after deductions awarded on defendant's counterclaim, of $4,367.01.</p>\n<p>Upon exceptions duly filed and heard at the December Term, 1927, Moore Superior Court, the same were overruled, and the referee's findings of fact and conclusions of law were adopted and approved, from which judgment the defendant appeals, assigning errors.\nIt is settled by all the authorities that the findings of fact, made by a referee and approved by the trial judge, are not subject to review on appeal, if they are supported by any competent evidence. <em>Dorsey v. Mining</em><em>Co.</em>, <cross_reference><span class=\"citation no-link\"><span class=\"volume\">177</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">60</span></span></cross_reference>, <cross_reference><span class=\"citation\" data-id=\"3644667\"><a href=\"/opinion/3898707/dorsey-v-mining-co/\"><span class=\"volume\">97</span> <span class=\"reporter\">S.E.</span> <span class=\"page\">746</span></a></span></cross_reference>. Likewise, where the judge, upon hearing and considering exceptions to a referee's report, makes different or additional findings of fact, they afford no ground for exception on appeal, unless there is no sufficient evidence to support them, or error has been committed in receiving or rejecting testimony on which they are based, or some other question of law is raised with respect to said findings. <em>Kenney</em><em>v. Hotel Co.</em>, <cross_reference><span class=\"citation\" data-id=\"3669520\"><a href=\"/opinion/3922987/kenney-v-hotel-co/\"><span class=\"volume\">194</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">44</span></a></span></cross_reference>, <cross_reference><span class=\"citation\" data-id=\"3669520\"><a href=\"/opinion/3922987/kenney-v-hotel-co/\"><span class=\"volume\">138</span> <span class=\"reporter\">S.E.</span> <span class=\"page\">349</span></a></span></cross_reference>; <em>S. v. Jackson</em>, <cross_reference><span class=\"citation no-link\"><span class=\"volume\">183</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">695</span></span></cross_reference>,<cross_reference><span class=\"citation\" data-id=\"3654635\"><a href=\"/opinion/3908442/state-v-jackson/\"><span class=\"volume\">110</span> <span class=\"reporter\">S.E.</span> <span class=\"page\">593</span></a></span></cross_reference>.</p>\n<p>Applying this rule, it would seem that the exceptions of appellant should be overruled and the judgment of the Superior Court affirmed.</p>\n<p>The defendant's brief is devoted largely to a discussion of the question as to whether this suit can be maintained by the plaintiff because of an alleged disregard of the following stipulation in the contract between the parties: <span class=\"star-pagination\">*Page 616</span> </p>\n<p>\"In case of a contention where amicable settlement cannot be reached, Mr. G. W. McKibbin shall be considered the arbiter, and in the event that his services cannot be obtained, some other architect shall take his place who meets with the approval of the contractor and the owners. In the case of a contention of this sort, the expenses incurred shall be paid by the parties who are found to be at fault.\"</p>\n<p>It is the contention of the defendant that as the plaintiff made no effort to settle the matters in dispute by arbitration, as the contract provides, before bringing suit, the same should be dismissed on authority of what was said in <em>Webb v. Trustees</em>, <cross_reference><span class=\"citation\" data-id=\"3661845\"><a href=\"/opinion/3915483/webb-v-trustees/\"><span class=\"volume\">143</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">299</span></a></span></cross_reference>, <cross_reference><span class=\"citation\" data-id=\"3661845\"><a href=\"/opinion/3915483/webb-v-trustees/\"><span class=\"volume\">55</span> <span class=\"reporter\">S.E.</span> <span class=\"page\">719</span></a></span></cross_reference>, and<em>Young v. Jeffreys</em>, <cross_reference><span class=\"citation no-link\"><span class=\"volume\">20</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">357</span></span></cross_reference>. In answer to this position, we deem it sufficient to say that the matter seems to have been waived, even if originally formidable, which may be doubted. <em>Williams v. Mfg. Co.</em>,<cross_reference><span class=\"citation\" data-id=\"3644530\"><a href=\"/opinion/3898577/williams-v-manufacturing-co/\"><span class=\"volume\">154</span> <span class=\"reporter\">N.C.</span> <span class=\"page\">205</span></a></span></cross_reference>, <cross_reference><span class=\"citation\" data-id=\"3644530\"><a href=\"/opinion/3898577/williams-v-manufacturing-co/\"><span class=\"volume\">70</span> <span class=\"reporter\">S.E.</span> <span class=\"page\">290</span></a></span></cross_reference>.</p>\n<p>A careful perusal of the record leaves us with the impression that the case has been tried substantially in accord with the principles of law applicable, and that the judgment should be upheld.</p>\n<p>Affirmed.</p>", "CL_opinion_per_curiam": false}