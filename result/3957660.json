luster_precedential_status": "Published", "CL_cluster_case_name_short": "Weems", "CL_cluster_sub_opinions": ["http://www.courtlistener.com/api/rest/v3/opinions/3957661/", "http://www.courtlistener.com/api/rest/v3/opinions/3957662/", "http://www.courtlistener.com/api/rest/v3/opinions/3957660/"], "CL_cluster_absolute_url": "/opinion/4188190/weems-v-state/", "CL_cluster_scdb_decision_direction": null, "CL_cluster_westlaw_cite": "", "CL_cluster_state_cite_one": "148 Tex. Crim. 154", "CL_cluster_procedural_history": "", "CL_cluster_date_blocked": null, "CL_cluster_specialty_cite_one": "", "CL_cluster_case_name_full": "Charles Sumner Weems v. State", "CL_cluster_date_filed_is_approximate": false, "CL_cluster_nature_of_suit": "", "CL_cluster_federal_cite_two": "", "CL_cluster_scotus_early_cite": "", "CL_cluster_date_filed": "1944-11-15", "CL_cluster_federal_cite_one": "", "CL_cluster_citation_id": null, "CL_cluster_source": "Z", "CL_cluster_scdb_id": "", "CL_cluster_syllabus": "", "CL_cluster_panel": [], "CL_cluster_federal_cite_three": "", "CL_cluster_posture": "Appeal from Criminal District Court No. 1, Dallas County. Hon. J. Frank Wilson, Special Judge.\n\nAppeal from conviction for murder; penalty, confinement in the penitentiary for fifteen years.\n\n Affirmed. \n\nThe opinion states the case.", "CL_cluster_case_name": "Weems v. State", "CL_cluster_non_participating_judges": [], "CL_cluster_state_cite_regional": "185 S.W.2d 431", "CL_cluster_blocked": false, "CL_cluster_state_cite_three": "", "CL_cluster_resource_uri": "http://www.courtlistener.com/api/rest/v3/clusters/4188190/", "CL_cluster_date_created": "2016-07-06T10:18:35.291364Z"}, "Opinion_Date": "1944-11-15 00:00:00", "ML_Tags_Text": [], "Cases_Cited_Link": [], "CL_opinion_absolute_url": "/opinion/4188190/weems-v-state/", "CL_opinion_extracted_by_ocr": false, "Rule_Tags": "", "Manual_Tags": [], "Title": "Weems v. State", "PartyType1": "", "Document_Level_Tags": ["ScrapeSource_CL", "ScrapeDate_06202018"], "Case_Text": "", "TF_IDF_Tags": [], "ML_Tags_Doc_Level": [], "Data_source": "Court Listener", "CL_opinion_type": "020lead", "Duplicate": "No", "HTMLtext": "<p>This is an appeal from a conviction for murder, with punishment assessed at fifteen years in the penitentiary.</p>\n<p>The record suggests that appellant is at liberty, pending his appeal. No valid recognizance or appeal bond appears in the record.</p>\n<p>Under such circumstances, this Court is without jurisdiction to entertain the appeal.</p>\n<p>The appeal is therefore dismissed.</p>\n<p>Appellant is allowed fifteen days from and after this date within which to file and have approved a proper appeal bond.</p>\n<p>The foregoing opinion of the Commission of Appeals has been examined by the Judges of the Court of Criminal Appeals and approved by the Court.</p>\n<p><center>                         ON THE MERITS.</center></p>", "CL_opinion_per_curiam": false}