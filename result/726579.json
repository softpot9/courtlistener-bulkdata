wo": "", "CL_cluster_nature_of_suit": "", "CL_cluster_scotus_early_cite": "", "CL_cluster_date_filed": "1996-09-19", "CL_cluster_federal_cite_one": "95 F.3d 1335", "CL_cluster_citation_id": 699334, "CL_cluster_scdb_id": "", "CL_cluster_syllabus": "", "CL_cluster_panel": [], "CL_cluster_federal_cite_three": "", "CL_cluster_posture": "", "CL_cluster_case_name": "United States v. Harold Leon McGhee", "CL_cluster_non_participating_judges": [], "CL_cluster_source": "R", "CL_cluster_blocked": false, "CL_cluster_state_cite_three": "", "CL_cluster_resource_uri": "http://www.courtlistener.com/api/rest/v3/clusters/726579/", "CL_cluster_date_created": "2014-12-22T15:39:38.362567Z"}, "Opinion_Date": "1996-09-19 00:00:00", "ML_Tags_Text": [], "Cases_Cited_Link": ["http://www.courtlistener.com/api/rest/v3/opinions/720577/", "http://www.courtlistener.com/api/rest/v3/opinions/726578/"], "CL_opinion_absolute_url": "/opinion/726579/united-states-v-harold-leon-mcghee/", "CL_opinion_extracted_by_ocr": false, "Rule_Tags": "", "Manual_Tags": [], "Title": "United States v. Harold Leon McGhee", "PartyType1": "", "Document_Level_Tags": ["ScrapeSource_CL", "ScrapeDate_06202018"], "Case_Text": "", "TF_IDF_Tags": [], "ML_Tags_Doc_Level": [], "Data_source": "Court Listener", "CL_opinion_type": "010combined", "Duplicate": "No", "HTMLtext": "<p class=\"case_cite\"><span class=\"citation\" data-id=\"726578\"><a href=\"/opinion/726578/cally-kardules-mayor-village-of-new-albany-ohio-emery-bennett-david/\"><span class=\"volume\">95</span> <span class=\"reporter\">F.3d</span> <span class=\"page\">1335</span></a></span></p>\n    <p class=\"parties\">UNITED STATES of America, Plaintiff-Appellee,<br>v.<br>Harold Leon McGHEE, Defendant-Appellant.</p>\n    <p class=\"docket\">No. 95-6323.</p>\n    <p class=\"court\">United States Court of Appeals,<br>Sixth Circuit.</p>\n    <p class=\"date\">Sept. 19, 1996.</p>\n    <div class=\"prelims\">\n      <p class=\"indent\">Prior report:  <span class=\"citation\" data-id=\"720577\"><a href=\"/opinion/720577/united-states-v-harold-leon-mcghee/\"><span class=\"volume\">87</span> <span class=\"reporter\">F.3d</span> <span class=\"page\">184</span></a></span>.</p>\n      <p class=\"indent\">Before:  MERRITT, Chief Judge;  KENNEDY, MARTIN, NELSON, RYAN, BOGGS, NORRIS, SUHRHEINRICH, SILER, BATCHELDER, DAUGHTREY, MOORE and COLE, Circuit Judges.</p>\n    </div>\n    <p>ORDER</p>\n    <div class=\"num\" id=\"p1\">\n      <span class=\"num\">1</span>\n      <p class=\"indent\">A majority of the Judges of this Court in regular active service have voted for rehearing of this case en banc.  Sixth Circuit Rule 14 provides as follows:</p>\n    </div>\n    <div class=\"num\" id=\"p2\">\n      <span class=\"num\">2</span>\n      <p>The effect of the granting of a hearing en banc shall be to vacate the previous opinion and judgment of this court, to stay the mandate and to restore the case on the docket sheet as a pending appeal.</p>\n    </div>\n    <div class=\"num\" id=\"p3\">\n      <span class=\"num\">3</span>\n      <p class=\"indent\">Accordingly, it is ORDERED that the previous decision and judgment of this court are vacated, the mandate is stayed and the case is restored to the docket as a pending appeal.</p>\n    </div>\n    <div class=\"num\" id=\"p4\">\n      <span class=\"num\">4</span>\n      <p class=\"indent\">It is further ORDERED that the appellant file a supplemental brief not later than Monday, October 21, 1996, and the appellee file a supplemental brief not later than Monday, December 23, 1996.  Reargument will be scheduled for Wednesday, March 12, 1997.</p>\n    </div>\n    ", "CL_opinion_per_curiam": false}